#include<bits/stdc++.h>
#define ll long long
#define pb push_back
#define mp make_pair

using namespace std;

int main()
{
    ll t;
    cin>>t;
    while(t--)
    {
        ll n,i,k=0,sum=0,m,ma=0,mi=0,l1,p,q,r2,a,b,c,d,x,df=0;
        vector<int> v,v2;

      cin>>n;
      for(i=0;i<n;i++)
      {
          cin>>x;
          v.push_back(x);
      }
      for(i=0;i<n;i++)
      {
          cin>>x;
          v2.push_back(x);
      }
      cout<<v2[0]-v[0]<<" ";
       for(i=1;i<n;i++)
       {
           if(v[i]>v2[i-1])
           {
               cout<<v2[i]-v[i]<<" ";
           }
           else
            cout<<v2[i]-v2[i-1]<<" ";
       }
       cout<<endl;
    }


}
